(1
 (simple-single .
                [(1 3)
                 nil "A single-file package with no dependencies" single])
 (simple-depend .
                [(1 0)
                 ((simple-single (1 3))) "A single-file package with a dependency." single])
 (multi-file .
             [(0 2 3)
              nil "Example of a multi-file tar package" tar]))
